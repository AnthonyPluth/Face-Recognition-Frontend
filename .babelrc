{
  "presets":[
     "@babel/preset-react"
  ],
  "env":{
     "test":{
        "plugins":[
           [
              "istanbul",
              {
               "include":[
                  "src/**/*.{js,jsx}"
               ],
                 "exclude":[
                    "**/*.spec.{js,jsx}"
                 ]
              }
           ]
        ]
     }
  },
  "plugins":[
     "transform-class-properties"
  ]
}